<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.hetao.dao.UserDao">
    <select id="isUser" resultType="org.hetao.domain.User">
        select user.id, user.user_id,user.password from user where user.user_id=#{user_id} and user.password=#{password};
    </select>
    <update id="">

    </update>
</mapper>